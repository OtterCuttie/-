#include <iostream>
using namespace std;

int main() {
	setlocale(0, "");
	int n;
	int k=0;
	cout << "Цена игрушки:";
	cin >> n;

	int a = n / 50;
	int b = n / 100;
	int c = n / 200;
	for (int i = 0; i < a+1; i += 1) {
		for (int j = 0; j < b+1; j += 1) {
			for (int h = 0; h < c+1; h += 1) {
				int l;
				l = 50 * i + 100 * j + 200 * h;
				if (l != 0) {
					if (n==l) {
						k = k + 1;
					}
				}
					
				



			}

		}

	}
	cout << k;
}
